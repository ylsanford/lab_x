<h1>Listing ordered_tests</h1>

<table>
  <thead>
    <tr>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordered_tests.each do |ordered_test| %>
      <tr>
        <td><%= ordered_test.patient %></td>
        <td><%= link_to 'Show', ordered_test %></td>
        <td><%= link_to 'Edit', edit_ordered_test_path(ordered_test) %></td>
        <td><%= link_to 'Destroy', ordered_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ordered test', new_ordered_test_path %>
